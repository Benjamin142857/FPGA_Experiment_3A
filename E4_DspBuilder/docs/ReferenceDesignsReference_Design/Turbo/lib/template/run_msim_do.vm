#include("alt_license_tcl.txt")

#set ($hash = "#")
$hash$hash Generated by: $tool
$hash$hash Generated on: $date

if [file exists work] {
  vdel -all
}

vlib work
vlib stratix
vlib cyclone
vlib stratixii
vlib cycloneii
vmap work work
vmap lpm work
vmap altera_mf work
vmap stratix stratix
vmap cyclone cyclone
vmap stratixii stratixii
vmap cycloneii cycloneii
vcom -93 -work work  $quartus_rootdir/eda/sim_lib/altera_mf_components.vhd
vcom -93 -work work  $quartus_rootdir/eda/sim_lib/altera_mf.vhd
vcom -93 -work work  $quartus_rootdir/eda/sim_lib/220pack.vhd
vcom -93 -work work  $quartus_rootdir/eda/sim_lib/220model.vhd
vcom -93 -work stratix  $quartus_rootdir/eda/sim_lib/stratix_atoms.vhd
vcom -93 -work stratix  $quartus_rootdir/eda/sim_lib/stratix_components.vhd
vcom -93 -work cycloneii  $quartus_rootdir/eda/sim_lib/cycloneii_atoms.vhd
vcom -93 -work cycloneii  $quartus_rootdir/eda/sim_lib/cycloneii_components.vhd
#foreach ($file in $files)
vcom -93 -work work  $file
#end

if [file exists "wave.do"] {
  do wave.do
} elseif [file exists "wave_default.do"] {
  do wave_default.do
} 
